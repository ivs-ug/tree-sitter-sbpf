{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sbpf",
      "camelcase": "sBPF",
      "title": "sBPF",
      "scope": "source.sbpf",
      "file-types": [
        "s",
        "out",
        "S"
      ],
      "injection-regex": "^sbpf$",
      "class-name": "TreeSitterSbpf",
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Solana sBPF Assembly",
    "authors": [
      {
        "name": "Ivan Sidorov",
        "email": "ivansid5.kz@proton.me"
      }
    ],
    "links": {
      "repository": "https://github.com/ivs-ug/tree-sitter-sbpf"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
